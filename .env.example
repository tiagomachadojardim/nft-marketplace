# Environment Variables Example
# Copy this file to .env and fill in your values

# App Configuration
REACT_APP_NAME=NFT Marketplace
REACT_APP_VERSION=0.1.0

# API Configuration (if needed in the future)
# REACT_APP_API_URL=https://api.example.com
# REACT_APP_API_KEY=your_api_key_here

# Social Media Links
REACT_APP_DISCORD_URL=https://discord.com
REACT_APP_YOUTUBE_URL=https://www.youtube.com
REACT_APP_TWITTER_URL=https://twitter.com
REACT_APP_INSTAGRAM_URL=https://www.instagram.com

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
